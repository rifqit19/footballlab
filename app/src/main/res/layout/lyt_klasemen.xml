            android:textColor="@color/black"
            android:textSize="@dimen/text_small_size"
            android:fontFamily="@font/helveticaneue_medium"
            android:textAlignment="center"
            android:padding="@dimen/margin_extra_small"/>
        <TextView
            android:id="@+id/l"
